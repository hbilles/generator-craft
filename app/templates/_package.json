{
  "name": "<%= domainName %>",
  "description": "Website code repo for <%= siteName %>",
  "repository": {
    "type": "git",
    "url": "https://git@bitbucket.org/line58/<%= domainName %>.git"
  },
  "version": "0.0.1",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "fitVids": "./src/js/vendor/jquery.fitvids.js",
    "wolfy87-eventemitter": "./node_modules/imagesloaded/node_modules/wolfy87-eventemitter/EventEmitter.js",
    "eventie": "./node_modules/imagesloaded/node_modules/eventie/eventie.js",
    "imagesLoaded": "./node_modules/imagesloaded/imagesloaded.js",
    "matchHeight": "./src/js/vendor/jquery.matchHeight.js",
    "owlCarousel": "./node_modules/owl.carousel/dist/owl.carousel.js",
    "placeholder": "./src/js/vendor/jquery.placeholder.js",
    "validate": "./src/js/vendor/jquery.validate.js",
    "validateAdditional": "./src/js/vendor/jquery.validate.additional.js"
  },
  "browserify-shim": {
    "owlCarousel": {
      "depends": [
        "jquery:window.jQuery"
      ]
    },
    "fitVids": {
      "depends": [
        "jquery:window.jQuery"
      ]
    },
    "matchHeight": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "placeholder": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "validate": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "validateAdditional": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~2.7.10",
    "browserify": "~8.1.3",
    "browserify-shim": "~3.8.8",
    "gulp": "~3.9.0",
    "gulp-autoprefixer": "~2.3.1",
    "gulp-changed": "~1.2.1",
    "gulp-minify-css": "~1.1.6",
    "gulp-notify": "~2.2.0",
    "gulp-open": "~0.3.2",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "~2.0.1",
    "gulp-shell": "^0.4.2",
    "gulp-streamify": "~0.0.5",
    "gulp-uglify": "~1.2.0",
    "gulp-util": "~3.0.5",
    "node-bourbon": "~4.2.3",
    "node-neat": "^1.7.2",
    "pretty-hrtime": "~1.0.0",
    "vinyl-source-stream": "~1.0.0",
    "vinyl-transform": "~1.0.0",
    "watchify": "~3.2.2"
  },
  "dependencies": {
    "imagesLoaded": "desandro/imagesloaded",
    "jquery": "^1.11.1",
    "owlCarousel": "OwlFonk/OwlCarousel2",
    "respond": "scottjehl/Respond"
  }
}
